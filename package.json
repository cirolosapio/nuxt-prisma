{
  "name": "nuxt-app",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "sql:generate": "npx prisma migrate dev --name init"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "eslint": "^8.48.0",
    "nuxt": "^3.7.0",
    "prisma": "^5.2.0"
  },
  "dependencies": {
    "@prisma/client": "5.2.0"
  },
  "eslintConfig": {
    "extends": "@nuxtjs/eslint-config-typescript",
    "rules": {
      "curly": [
        "error",
        "multi-or-nest"
      ],
      "semi": [
        "error",
        "never"
      ]
    }
  }
}